{
  "version": 2,
  "buildCommand": "cd client && npm run vercel-build",
  "outputDirectory": "client/dist",
  "installCommand": "npm run install-all",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 